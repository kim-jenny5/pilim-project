<div class="container my-auto form_container">
  <div class="row">
    <h3 class="page_title">Create a New List</h3>
  </div>
  <div class="container">
    <div class="row">
      <div class="col nopadding">
        <%= render partial: "application/error_messages", locals: { term: @list } %>
      </div>
    </div>
    <%= form_for @list do |f| %>
      <%= render partial: "form", locals: { f: f } %>
      <div class="row">
        <div class="col nopadding">
          <%= f.submit "Submit", class: "btn btn-custom" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
